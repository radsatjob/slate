  </span><span class="nl">"U"</span><span class="p">:</span><span class="w"> </span><span class="mi">3960811287130</span><span class="p">,</span><span class="w">  </span><span class="err">//</span><span class="w"> </span><span class="err">Final</span><span class="w"> </span><span class="err">update</span><span class="w"> </span><span class="err">ID</span><span class="w"> </span><span class="err">in</span><span class="w"> </span><span class="err">event</span><span class="w">
  </span><span class="nl">"u"</span><span class="p">:</span><span class="w"> </span><span class="mi">3960811297623</span><span class="p">,</span><span class="w">  </span><span class="err">//</span><span class="w"> </span><span class="err">First</span><span class="w"> </span><span class="err">update</span><span class="w"> </span><span class="err">ID</span><span class="w"> </span><span class="err">in</span><span class="w"> </span><span class="err">event</span><span class="w">
  </span><span class="nl">"pu"</span><span class="p">:</span><span class="w"> </span><span class="mi">3960811281312</span><span class="p">,</span><span class="w">  </span><span class="err">//</span><span class="w"> </span><span class="err">Previous</span><span class="w"> </span><span class="err">final</span><span class="w"> </span><span class="err">update</span><span class="w"> </span><span class="err">ID</span><span class="w">
  </span><span class="nl">"b"</span><span class="p">:</span><span class="w"> </span><span class="p">[</span><span class="w"> </span><span class="err">//</span><span class="w"> </span><span class="err">Bids</span><span class="w"> </span><span class="err">to</span><span class="w"> </span><span class="err">be</span><span class="w"> </span><span class="err">updated</span><span class="w">
    </span><span class="p">[</span><span class="w"> 
      </span><span class="s2">"4240107.8"</span><span class="p">,</span><span class="w"> </span><span class="err">//</span><span class="w"> </span><span class="err">Price</span><span class="w"> </span><span class="err">Value</span><span class="w">
      </span><span class="s2">"0.710"</span><span class="w"> </span><span class="err">//</span><span class="w"> </span><span class="err">Quantity</span><span class="w">
