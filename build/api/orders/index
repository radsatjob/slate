<span class="nx">In</span> <span class="k">this</span> <span class="k">case</span><span class="p">,</span> <span class="nx">shift</span> <span class="nx">the</span> <span class="nx">upper</span> <span class="nx">bound</span><span class="p">,</span> <span class="nx">i</span><span class="p">.</span><span class="nx">e</span><span class="p">.</span> <span class="nx">nextTimestamp</span> <span class="nx">value</span> <span class="nx">should</span> <span class="nx">be</span> <span class="nx">entered</span> <span class="k">in</span> <span class="nx">the</span> <span class="nx">endTimestamp</span> <span class="nx">query</span> <span class="nx">param</span> <span class="k">for</span> <span class="nx">the</span> <span class="nx">next</span> <span class="nx">API</span> <span class="nx">call</span><span class="p">.</span>
